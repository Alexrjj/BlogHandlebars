<div class="container">
  <br>
  {{#each posts}}
    <div class="card">
    <div class="card-body">
      <h2 class="card-title">{{this.title}}</h2>
      <p class="card-subtitle text-muted">{{this.name}}{{createdAt}}</p>
      <p class="card-text">{{this.text}}</p>
      <a href="#" class="card-link">Ler mais...</a>
    </div>
    <br>
  </div>
  {{/each}}
</div>